<avalonia:NavigationStackView 
	xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:views="clr-namespace:PelicanExample.Views"
  xmlns:avalonia="clr-namespace:Pelican.Avalonia;assembly=Pelican.Avalonia"
  mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="700"
  x:Class="PelicanExample.Views.MainView"
	Router="{Binding Router}"
	x:DataType="views:MainViewModel"
>
	<avalonia:NavigationStackView.DataContext>
		<views:MainViewModel/>
	</avalonia:NavigationStackView.DataContext>
</avalonia:NavigationStackView>
