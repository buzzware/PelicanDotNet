<app:NavigationStackView 
	xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:app="clr-namespace:AvaloniaCrossApp"
	mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="700"
  x:Class="AvaloniaCrossApp.MainView"
	Router="{Binding Router}"
	x:DataType="app:MainViewModel"
>
	<app:NavigationStackView.DataContext>
		<app:MainViewModel/>
	</app:NavigationStackView.DataContext>
</app:NavigationStackView>
