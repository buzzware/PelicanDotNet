<pelican:NavigationStackView 
	xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:views="clr-namespace:PelicanExample.Views"
  xmlns:pelican="clr-namespace:Pelican.Avalonia;assembly=Pelican.Avalonia"
  mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="400"
  x:Class="PelicanExample.Views.MainView"
	x:DataType="views:MainViewModel"
	HorizontalAlignment="Stretch"
	VerticalAlignment="Stretch"
>
	<pelican:NavigationStackView.Content>
		<Grid ColumnDefinitions="*,*">
			<Rectangle Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Fill="Green"/>
			<Rectangle Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Fill="Yellow"/>
		</Grid>
	</pelican:NavigationStackView.Content>
</pelican:NavigationStackView>



